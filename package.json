{
  "name": "drawo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test:drawer:unit": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 nodemon --exec 'yarn mocha' --watch plugins -e ts,js",
    "test:drawer:benchmark": "vite -c plugins/drawer/tests/benchmark/vite.config.ts",
    "test:drawer:integration:server": "vite -c plugins/drawer/tests/integration/web/vite.config.ts",
    "test:drawer:integration:start": "cypress open",
    "test:drawer:integration": "start-server-and-test test:drawer:integration:server http://localhost:4320 test:drawer:integration:start",
    "dev": "vite",
    "build": "TS_NODE_PROJECT=tsconfig.build.json vue-tsc -p tsconfig.build.json && vite build",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "server": "TS_NODE_PROJECT=server/tsconfig.json nodemon --exec node -P server/tsconfig.json --loader ts-paths-esm-loader --experimental-specifier-resolution=node --watch server --watch plugins server/index.ts"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vuelidate/core": "^2.0.0",
    "@vuelidate/validators": "^2.0.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-vue": "^9.8.0",
    "nanoid": "^4.0.0",
    "prettier": "^2.8.2",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.5.4",
    "vite-svg-loader": "^4.0.0",
    "vue": "^3.2.45",
    "vue-router": "4",
    "vue-socket.io": "^3.0.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.21.0",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-vite": "^0.2.7",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.15",
    "@types/cypress": "^1.1.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "babel-loader": "^8.3.0",
    "chai": "^4.3.7",
    "eslint-plugin-cypress": "^2.12.1",
    "esm": "^3.2.25",
    "mocha": "^10.2.0",
    "node-sass": "^8.0.0",
    "nodemon": "^2.0.20",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "ts-paths-esm-loader": "^1.4.1",
    "tsconfig-paths": "^3.14.2",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^1.0.11"
  },
